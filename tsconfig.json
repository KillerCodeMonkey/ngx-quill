{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "esnext",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "es2022",
    "useDefineForClassFields": false,
    "lib": ["es2018", "dom"],
    "paths": {
      "itopplus-ngx-quill/config": [
        "projects/itopplus-ngx-quill/config/src/public_api.ts"
      ],
      "itopplus-ngx-quill": ["projects/itopplus-ngx-quill/src/public_api.ts"]
    }
  },
  "angularCompilerOptions": {
    "fullTemplateTypeCheck": true,
    "strictInjectionParameters": true
  }
}
