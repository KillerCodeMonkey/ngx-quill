{
  "name": "ngx-quill",
  "license": "MIT",
  "version": "8.1.3",
  "author": {
    "name": "Bengt Weiße"
  },
  "description": "An angular component for the easy use of the QuillJS richt text editor.",
  "bugs": "https://github.com/KillerCodeMonkey/ngx-quill/issues",
  "homepage": "https://github.com/KillerCodeMonkey/ngx-quill",
  "repository": {
    "type": "git",
    "url": "https://github.com/KillerCodeMonkey/ngx-quill"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "lint": "eslint src/**/**/*.ts src/**/*.ts",
    "build": "ng-packagr -p package.json",
    "pretest": "npm run lint",
    "test": "jest",
    "test:ci": "jest --runInBand"
  },
  "keywords": [
    "editor",
    "rich text",
    "wysiwyg",
    "angular",
    "directive",
    "component",
    "quill",
    "quilljs",
    "angular",
    "ng",
    "ngx",
    "text editor"
  ],
  "main": "./bundles/index.js",
  "module": "./src/quill.module.js",
  "typings": "index.d.ts",
  "ngPackage": {
    "lib": {
      "entryFile": "index.ts"
    }
  },
  "browserslist": [
    "defaults"
  ],
  "$schema": "./node_modules/ng-packagr/package.schema.json",
  "peerDependencies": {
    "@angular/animations": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/common": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/core": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/forms": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "@angular/platform-browser": "^7.0.0 || ^8.0.0 || ^9.0.0",
    "quill": "^1.3.7",
    "rxjs": "^6.4.0 || ^6.5.0"
  },
  "devDependencies": {
    "@angular/animations": "^9.0.0",
    "@angular/common": "^9.1.0",
    "@angular/compiler": "^9.1.0",
    "@angular/compiler-cli": "^9.1.0",
    "@angular/core": "^9.1.0",
    "@angular/forms": "^9.1.0",
    "@angular/platform-browser": "^9.1.0",
    "@angular/platform-browser-dynamic": "^9.1.0",
    "@types/jest": "^25.2.1",
    "@types/node": "^13.9.4",
    "@types/quill": "^1.3.10",
    "@typescript-eslint/eslint-plugin": "^2.25.0",
    "@typescript-eslint/parser": "^2.25.0",
    "ajv": "^6.12.0",
    "canvas": "^2.6.1",
    "codelyzer": "^5.2.1",
    "core-js": "^3.6.4",
    "eslint": "^6.8.0",
    "jest": "^25.3.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "jest-preset-angular": "^8.1.3",
    "loader-utils": "^2.0.0",
    "ng-packagr": "^9.1.0",
    "quill": "^1.3.7",
    "reflect-metadata": "^0.1.13",
    "rollup": "^1.20.0",
    "rxjs": "^6.5.4",
    "source-map-loader": "^0.2.4",
    "ts-helpers": "^1.1.2",
    "ts-loader": "7.0.0",
    "tsickle": "0.38.1",
    "tslib": "^1.11.1",
    "tslint": "^5.20.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "~3.8.3",
    "webpack": "^4.42.1",
    "webpack-cli": "^3.3.11",
    "webpack-node-externals": "^1.7.2",
    "zone.js": "~0.10.3"
  },
  "jest": {
    "preset": "jest-preset-angular",
    "setupFilesAfterEnv": [
      "<rootDir>/src/setupJest.ts"
    ],
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "collectCoverage": true,
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80
      }
    }
  },
  "contributors": [
    {
      "name": "Bengt Weiße"
    }
  ]
}
